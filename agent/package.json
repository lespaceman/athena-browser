{
  "name": "athena-agent",
  "version": "1.0.0",
  "description": "AI-powered agent for Athena Browser with MCP tool integration",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "build": "tsup",
    "build:tsc": "tsc",
    "dev": "tsx watch src/server.ts",
    "start": "node dist/server.js",
    "start:mcp": "node dist/mcp-stdio-server.js",
    "dev:mcp": "tsx watch src/mcp-stdio-server.ts",
    "mcp:inspect": "npx @modelcontextprotocol/inspector node dist/mcp-stdio-server.js",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:integration": "vitest run test/integration",
    "test:mcp": "vitest run test/mcp"
  },
  "keywords": [
    "athena",
    "browser",
    "ai",
    "claude",
    "mcp",
    "agent"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/claude-agent-sdk": "latest",
    "@modelcontextprotocol/sdk": "^1.0.4",
    "express": "^4.19.2",
    "winston": "^3.14.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "@types/express": "^4.17.21",
    "@types/node": "^20.14.9",
    "@typescript-eslint/eslint-plugin": "^7.16.0",
    "@typescript-eslint/parser": "^7.16.0",
    "eslint": "^8.57.1",
    "globals": "^16.4.0",
    "tsup": "^8.5.0",
    "tsx": "^4.16.2",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.46.2",
    "vitest": "^2.0.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
